<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>test_example.servlet.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/myservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FirstPageServlet</servlet-name>
        <servlet-class>test_example.servlet.FirstPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstPageServlet</servlet-name>
        <url-pattern>/firstpageservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>All parameters Servlet</servlet-name>
        <servlet-class>test_example.servlet.AllParamServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>All parameters Servlet</servlet-name>
        <url-pattern>/param</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Multithreding Servlet</servlet-name>
        <servlet-class>test_example.servlet.MultithreadingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Multithreding Servlet</servlet-name>
        <url-pattern>/multithread</url-pattern>
    </servlet-mapping>
</web-app>